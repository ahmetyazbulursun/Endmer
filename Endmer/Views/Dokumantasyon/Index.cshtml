@using Endmer.Models.Entity
@model List<Tbl_Dokuman>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid py-4">
    <div class="row">
        <div class="col-12">
            <div class="card mb-4">

                <div class="card-header">
                    <h6 class="text-center fs-4">Dökümantasyon</h6>
                </div>

                <div class="card-body px-0 pt-0 pb-2">
                    <div class="table-responsive p-0">

                        @foreach (var x in Model)
                        {
                            <a href="/Dokumantasyon/DokumanDetay/@x.ID"><h3>@x.BASLIK</h3></a>
                        }

                    </div>
                </div>

            </div>
        </div>
    </div>
</div>