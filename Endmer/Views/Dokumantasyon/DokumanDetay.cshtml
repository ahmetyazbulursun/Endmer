@using Endmer.Models.Entity
@model List<Tbl_Dokuman>

@{
    ViewBag.Title = "DokumanDetay";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid py-4">
    <div class="row">
        <div class="col-12">
            <div class="card mb-4">

                <div class="card-body px-0 pt-0 pb-2">

                    @foreach (var x in Model)
                    {
                        <div class="container">
                            <p>@Html.Raw(x.ICERIK)</p>
                        </div>
                    }

                </div>
            </div>
        </div>
    </div>
</div>